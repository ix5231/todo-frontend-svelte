{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "jest",
    "genapi": "openapi-generator-cli generate -i ../todo-doc/api-schema/web-bff.json -g typescript-axios -o ./src/api/client",
    "genschema": "openapi-typescript  ../todo-doc/api-schema/web-bff.json --output src/api/schema.ts",
    "gendef": "swagger-cli bundle -r -o src/api/def.json ../todo-doc/api-schema/web-bff.json"
  },
  "devDependencies": {
    "@apidevtools/swagger-cli": "^4.0.4",
    "@babel/core": "^7.17.5",
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.3.1",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/svelte": "^3.0.3",
    "@testing-library/user-event": "^14.0.0-beta.11",
    "@tsconfig/svelte": "^2.0.1",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.5.1",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-svelte3": "^3.4.1",
    "jest": "^27.5.1",
    "msw": "^0.38.1",
    "openapi-types": "^10.0.0",
    "openapi-typescript": "^5.2.0",
    "postcss": "^8.4.7",
    "rollup": "^2.68.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.5",
    "svelte-jester": "^2.3.2",
    "svelte-loading-spinners": "^0.1.7",
    "svelte-preprocess": "^4.10.4",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "axios": "0.21.4",
    "sirv-cli": "^2.0.2",
    "svelte-spa-router": "^3.2.0"
  }
}